+package sandbox

[n] > fibonacci
  if. > @
    less.
      n
      2
    n
    subFibonacci
      sub.
        n
        2
      1
      0

[i last beforelast] > subFibonacci
  add. > current
    last
    beforelast
  if. > @
    greater.
      i
      0
    subFibonacci
      sub.
        i
        1
      current
      last
    current
